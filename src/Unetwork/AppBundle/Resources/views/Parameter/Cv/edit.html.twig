{% extends 'UnetworkAppBundle:Parameter:layout.html.twig' %}

{% block body %}

<h1>Modifier emploi actuel</h1>
	<div class="contentForm">

	{{ form_start(form, { 'attr': { 'role': 'form' }}) }}
	    {{ form_errors(form) }}


	    <table>
	    	<tbody>
	    		<tr class="form-group">
	    			<td>{{ form_label(form.presentation) }}</td>
	    			<td>{{ form_widget(form.presentation) }}</td>
	    		</tr>
	    		<tr class="form-group">
	    			<td>{{ form_label(form.jobname) }}</td>
	    			<td>{{ form_widget(form.jobname) }}</td>
	    		</tr>
	    		<tr class="form-group">
	    			<td><input type="submit" class="btn btn-default" /></td>
	    			<td></td>
	    		</tr>
	    	</tbody>
	    </table>
	    
	{{ form_end(form) }}

	</div>

{% endblock %}